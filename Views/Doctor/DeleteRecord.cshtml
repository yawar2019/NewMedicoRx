@model NewMedicoRx.Models.DoctorModel
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>DeleteRecord</title>
</head>
<body>
    <h3>Are you sure you want to delete this?</h3>
    <div>
        <h4>ReferalDoctorModel</h4>
        <hr />
        <b> @Html.DisplayNameFor(model => model.DoctorName):</b>

        <span>@Model.DoctorName</span>

        <br />
        <br />
        
        <form action="~/ReferalDoctor/DeleteConfirm/@Model.DocId" method="post">
            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-default" />
            </div>
        </form>
    </div>
    @Html.ActionLink("Back to List", "ReferDocIndex")

</body>
</html>
